// global constants 
(∧ ∨ ⇒ ⇔ ¬ ∀ ∃ =),{       

  // ND Prop rule library
  :{ :W :V  (∧ W V) }               \label{∧+}
  :{ :(∧ W V) W V }                 \label{∧-}
  :{ :W  (∨ W V) (∨ V W) }          \label{∨+}
  :{ :(∨ W V) :{:W U} :{:V U}  U }  \label{∨-}
  :{ :{:W V}  (⇒ W V) }             \label{⇒+}
  :{ :W :(⇒ W V)  V }               \label{⇒-}
  :{ :{:W V} :{:V W}  (⇒ W V) }     \label{⇔+}
  :{ :(⇔ W V)  {:W V} {:V W} }      \label{⇔-}
  :{ :{:W V (¬ V)}  (¬ W) }         \label{¬+}
  :{ :{:(¬ W) V (¬ V)}  W }         \label{¬-}
  
  // Altertnate or- proof
  {
    { :(∨ P Q) :(¬ Q)
      { :P
        P               \expectValidation{result}{"valid"} // freebie
      }
      { :Q
        { :(¬ P)
          (¬ Q)         \expectValidation{result}{"valid"} // freebie
        }
        P               \by{¬-}{W=P}{V=Q}
                        \expectValidation{result}{"valid"}
      }
      P                 \by{∨-}{W=P}{V=Q}{U=P}
                        \expectValidation{result}{"valid"}   
    }
   
  // Theorem (we should get this for free now... no justification needed)
  { :(∨ P Q) :(¬ Q) P }    \expectValidation{result}{"valid"}
}