// global constants 
(∧ ∨ ⇒ ⇔ ¬ ∀ ∃ =),{       

  // ND Prop rule library
  :{ :W :V  (∧ W V) }               \label{∧+}
  :{ :(∧ W V) W V }                 \label{∧-}
  :{ :W  (∨ W V) (∨ V W) }          \label{∨+}
  :{ :(∨ W V) :{:W U} :{:V U}  U }  \label{∨-}
  :{ :{:W V}  (⇒ W V) }             \label{⇒+}
  :{ :W :(⇒ W V)  V }               \label{⇒-}
  :{ :{:W V} :{:V W}  (⇒ W V) }     \label{⇔+}
  :{ :(⇔ W V)  {:W V} {:V W} }      \label{⇔-}
  :{ :{:W V (¬ V)}  (¬ W) }         \label{¬+}
  :{ :{:(¬ W) V (¬ V)}  W }         \label{¬-}
  
  // Baby warm up proof
  {
    { :P P }
    (⇒ P P )  \by{⇒+}{W=P}{V=P}
              \expectValidation{result}{"valid"}
  }
  
  // Theorem (we should get this for free now... no justification needed)
  (⇒ P P )    \expectValidation{result}{"valid"}
}